<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.eqca.repository.mapper.a.master.BizMapper">


	<select id="findAllBiz" resultType="MdBiz">
		SELECT
			biz_code,
			biz_type,
			biz_name,
			biz_desc,
			stock_level,
			stock_desc,
			proc_type
		FROM md_biz
	</select>

</mapper>

